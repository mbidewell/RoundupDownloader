<project name="ivydownload" xmlns:ivy="antlib:org.apache.ivy.ant" >
	<target name="ivy.fetch">
		<ivy:retrieve file="${ivy.dep.file}"
             pattern="${basedir}/distrib/[organisation]/[module]/[revision]/[type]/[artifact]-[revision].[ext]"/>
	</target>
	<target name="ivy.install"/>

</project>
